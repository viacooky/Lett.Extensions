{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": ["src/**.csproj"],
          "exclude": ["src/**Test.csproj"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["changelog/**.md", "changelog/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Lett.Extensions website",
      "_enableSearch": true,
      "_appFooter": "Copyright (c) - Lett.Extensions 2013 - 2016. All rights reserved.",
      "_disableContribution": true,
      "_enableNewTab": true
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "default(zh-cn)", "material/material"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": true,
    "keepFileLink": true,
    "cleanupCacheHistory": true,
    "disableGitFeatures": true
  }
}
